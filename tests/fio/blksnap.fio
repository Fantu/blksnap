[global]
ioengine=libaio
kb_base=1024
; directory=${DIR}
; filename_format=jobname.jobnumber.filenumber
direct=1
iodepth=8

[sequental_read]
rw=read
bs=1m
size=512m

[sequental_read_4k]
rw=read
bs=4k
size=128m

[sequental_write]
rw=write
bs=1m
size=512m

[sequental_write_4k]
rw=write
bs=4k
size=128m

[random_read]
rw=randread
bs=1m
size=512m

[random_read_4k]
rw=randread
bs=4k
size=128m

[random_write]
rw=randwrite
bs=1m
size=512m

[random_write_4k]
rw=randwrite
bs=4k
size=128m
